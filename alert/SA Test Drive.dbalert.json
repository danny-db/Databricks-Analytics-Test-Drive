{
  "evaluation": {
    "source": {
      "name": "cnt",
      "display": "cnt",
      "aggregation": "MAX"
    },
    "comparison_operator": "GREATER_THAN",
    "threshold": {
      "value": {
        "double_value": 1072.0
      }
    },
    "notification": {
      "retrigger_seconds": 0,
      "notify_on_ok": false
    }
  },
  "schedule": {
    "quartz_cron_schedule": "54 52 0 5 * ?",
    "timezone_id": "UTC"
  },
  "query_lines": [
    "-- Set up the alert using the following logic.",
    "SELECT",
    "  suburb,",
    "  COUNT(Total_Casualties) as cnt",
    "FROM",
    "  test_drive.road_crash.vw_crash_details",
    "WHERE suburb = 'ADELAIDE'",
    "GROUP BY",
    "  suburb;"
  ]
}
